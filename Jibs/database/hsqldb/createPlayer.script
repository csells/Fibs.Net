CREATE CACHED TABLE PLAYER(
	RATING DOUBLE,
	EXPERIENCE INTEGER,
	NAME VARCHAR(32) PRIMARY KEY,
	PASSWORD VARCHAR,
	EMAIL VARCHAR,
	ADMIN INTEGER,
	LAST_LOGIN_DATE TIMESTAMP,
	LAST_LOGOUT_DATE TIMESTAMP,
	LAST_LOGIN_HOST  VARCHAR,
	TOGGLE VARCHAR(20),
	BOARDSTYLE VARCHAR,
	LINELENGTH VARCHAR,
	PAGELENGTH VARCHAR,
	REDOUBLES VARCHAR,
	SORTWHO VARCHAR,
	TIMEZONE  VARCHAR
)







